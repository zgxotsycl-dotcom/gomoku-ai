{
  "scripts": {
    "build": "tsc",
    "start:pipeline": "node dist/start_pipeline.js",
    "start:status": "node dist/status_server.js",
    "start": "npm run build && node dist/start_pipeline.js",
    "start:one": "npm ci && npm run build && node scripts/run_or_docker.js",
    "docker": "docker compose up -d --build",
    "docker:gpu": "docker compose -f docker-compose.yml -f docker-compose.gpu.yml up -d --build",
    "docker:gpu:quick": "docker compose -f docker-compose.yml -f docker-compose.gpu.yml -f docker-compose.quick.yml up -d --build",
    "check:tf": "node scripts/check_tf_backend.js",
    "data:ingest:pro": "node dist/data/ingest_pro_games.js",
    "export:status": "node dist/monitor/export_status.js",
    "docker:gpu:workers": "docker compose -f docker-compose.yml -f docker-compose.gpu.yml -f docker-compose.gpu.workers.yml up -d --build",
    "pipeline:cycle": "node dist/scripts/pipeline_cycle.js",
    "data:ingest:psq": "node dist/data/ingest_psq_games.js",
    "experiments": "node dist/scripts/run_experiments.js",
    "data:ingest:psq:batch": "node dist/scripts/ingest_psq_batch.js",
    "report:status": "node dist/scripts/report_status.js",
    "analyze:psq": "node dist/scripts/analyze_psq.js",
    "curate:replay": "node dist/scripts/curate_replay_buffer.js",
    "check:inference": "node dist/scripts/check_inference.js",
    "tune:hparams": "node dist/scripts/tune_hparams.js",
    "automation:auto": "node dist/scripts/auto_research.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@google-cloud/storage": "^7.17.1",
    "@supabase/supabase-js": "^2.57.0",
    "@tensorflow/tfjs": "4.19.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fastify": "^4.28.1",
    "fs-extra": "^11.2.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "nanoid": "^5.1.5",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^15.7.2",
    "socket.io": "^4.8.1",
