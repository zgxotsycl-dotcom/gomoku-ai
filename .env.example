# Core sizes
BOARD_SIZE=15
RESIDUAL_BLOCKS=5
CONV_FILTERS=64
L2_REGULARIZATION=0.0001

# Pipeline toggles
RUN_DISTILLATION=true
UPLOAD_MODEL_AFTER=true
AUTO_GENERATE_OPENING_BOOK=true
IMPORT_OPENING_BOOK=true
PIPELINE_CYCLES=1
PIPELINE_INTERVAL_MS=0
FOREVER=true
ARENA_RESULT_PATH=arena_result.json
GATING_ENABLED=true
PRIORITIZE_RECENT_FILES=true
MAX_FILES_PER_EPOCH=0

# Distillation
BATCH_SIZE=64
EPOCHS=2
STEPS_PER_EPOCH=2000
LEARNING_RATE=0.001
TEACHER_TEMP=1.5
ALPHA_TEACHER_POLICY=0.7
BETA_TEACHER_VALUE=0.7

# Supabase
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_BUCKET=models
SUPABASE_MODEL_PREFIX=gomoku_model

# Server
PORT=8080
MODEL_URL= # optional: public Supabase model.json URL
MODEL_CHECK_INTERVAL_MS=300000
TIME_CONTROL=5+1

# Rules
OPENING_RULE=swap2

# Frontend
NEXT_PUBLIC_BOARD_SIZE=15
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# --- AI/MCTS & Heuristics ---
# TT/NN hybrid priors
TT_PRIOR_MIX=0.2
CHILD_TT_PRIOR_MIX=0.35

# Root tactical boosts
BOOST_CREATE=1.5
BOOST_BLOCK=1.3
BOOST_OPEN3_ROOT=1.08
BOOST_OPEN3_ROOT_BLOCK=1.05
BOOST_FOUR_ROOT=1.15
BOOST_FOUR_ROOT_BLOCK=1.1
BOOST_CONN3_ROOT=1.05
BOOST_CONN3_ROOT_BLOCK=1.03
BOOST_LINK_ROOT=1.03

# Child tactical boosts
BOOST_CREATE_CHILD=1.3
BOOST_BLOCK_CHILD=1.2
BOOST_OPEN3_CHILD=1.1
BOOST_OPEN3_BLOCK_CHILD=1.05
BOOST_CONN3_CHILD=1.05
BOOST_CONN3_BLOCK_CHILD=1.02
BOOST_LINK_CHILD=1.02
BOOST_INSTANT_WIN_CHILD=5.0
BOOST_BLOCK_IMMEDIATE_CHILD=2.0

# Swap2 candidate shaping
SWAP2_SYM_AVG=8
SWAP2_DIST_LAMBDA=0.0
SWAP2_DIST_PHASE_MOVES=8

# Auto tuning
TUNE_PARAMS_ON_ARENA=true
TUNE_LR=0.03

# TensorFlow backend selection
# Force CPU binding to avoid GPU dependency issues on Windows
TF_USE_GPU=0
# Optional: enable mixed precision when using GPU backend
TF_MIXED_PRECISION=0

# Gumroad / Supporters
GUMROAD_PRODUCT_ID_SUPPORTER=
GUMROAD_WEBHOOK_SECRET=
GUMROAD_PRODUCT_PERMALINK=
